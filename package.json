{
  "name": "fhir-client-mx",
  "version": "1.44.0",
  "description": "FHIR Client v3 - Electron Desktop Application",
  "main": "electron/main.js",
  "scripts": {
    "ng": "ng",
    "copy-monaco": "cpx \"node_modules/monaco-editor/min/vs/**/*\" \"public/assets/monaco/vs\"",
    "prestart": "npm run copy-monaco",
    "start": "concurrently \"ng serve\" \"wait-on http://localhost:4200 && electron .\"",
    "build": "ng build",
    "build:prod": "ng build --configuration production && electron-builder",
    "build:portable": "ng build --configuration production && electron-builder --win portable",
    "build:zip": "ng build --configuration production && electron-builder --win zip",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" --fix",
    "semantic-release": "semantic-release",
    "analyze": "ng build --configuration production --source-map && npx source-map-explorer dist/fhir-client/browser/*.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/martijn-on-fhir/fhir-client-v3.git"
  },
  "author": "Martijn Schimmel",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.2.0",
    "@angular/common": "^18.2.0",
    "@angular/compiler": "^18.2.0",
    "@angular/core": "^18.2.0",
    "@angular/forms": "^18.2.0",
    "@angular/platform-browser": "^18.2.0",
    "@angular/platform-browser-dynamic": "^18.2.0",
    "@angular/router": "^18.2.0",
    "@fortawesome/fontawesome-free": "^7.1.0",
    "@monaco-editor/loader": "^1.7.0",
    "@types/fhir": "^0.0.41",
    "@types/prismjs": "^1.26.5",
    "axios": "^1.13.2",
    "bootstrap": "^5.3.8",
    "electron-log": "^5.4.3",
    "electron-store": "^11.0.2",
    "fhirclient": "^2.6.3",
    "fhirpath": "^4.8.2",
    "handlebars": "^4.7.8",
    "monaco-editor": "^0.55.1",
    "node-forge": "^1.3.3",
    "otpauth": "^9.4.1",
    "prismjs": "^1.30.0",
    "qrcode": "^1.5.4",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "uuid": "^13.0.0",
    "vis-data": "^8.0.3",
    "vis-network": "^10.0.2",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.21",
    "@angular/cli": "^18.2.21",
    "@angular/compiler-cli": "^18.2.0",
    "@electron-forge/cli": "^7.11.1",
    "@electron-forge/maker-zip": "^7.11.1",
    "@eslint/js": "^9.39.2",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^12.0.2",
    "@semantic-release/npm": "^13.1.3",
    "@semantic-release/release-notes-generator": "^14.1.0",
    "@types/jasmine": "~5.1.0",
    "@types/node-forge": "^1.3.14",
    "@types/qrcode": "^1.5.6",
    "@typescript-eslint/eslint-plugin": "^8.52.0",
    "@typescript-eslint/parser": "^8.52.0",
    "concurrently": "^9.2.1",
    "cpx2": "^8.0.0",
    "electron": "^39.2.7",
    "electron-builder": "^26.4.0",
    "eslint": "^9.39.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsdoc": "^61.7.1",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "jasmine-core": "~5.2.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "semantic-release": "^25.0.2",
    "typescript": "~5.5.2",
    "typescript-eslint": "^8.52.0",
    "wait-on": "^9.0.3"
  }
}
